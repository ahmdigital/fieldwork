{
  "name": "sanity-plugin-fieldwork",
  "version": "1.0.0",
  "description": "Inspect your Sanity.io schema fields straight in your Studio using this tool",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmdigital/fieldwork.git"
  },
  "scripts": {
    "build": "babel src -d lib --copy-files",
    "lint": "eslint src",
    "test": "jest"
  },
  "keywords": [
    "cms",
    "content-model",
    "content",
    "headless",
    "modelling",
    "realtime",
    "sanity-io",
    "sanity-tool",
    "sanity",
    "tool",
    "schema",
    "fields"
  ],
  "author": "ahm digital (Cam Chamberlain)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ahmdigital/fieldwork/issues"
  },
  "dependencies": {
    "jest": "^26.0.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-icons": "^3.10.0",
    "styled-components": "^5.1.0"
  },
  "peerDependencies": {
    "@sanity/base": "^1.0.0"
  },
  "devDependencies": {
    "@ahmdigital/eslint-config": "3.1.33",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "babel-cli": "6.26.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "eslint": "7.0.0"
  },
  "renovate": {
    "extends": [
      "@ahmdigital"
    ]
  },
  "jest": {
    "resetMocks": true,
    "resetModules": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 39.13,
        "branches": 12.12,
        "lines": 35.94,
        "functions": 56
      }
    }
  }
}
